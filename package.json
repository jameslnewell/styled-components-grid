{
  "name": "styled-components-grid",
  "version": "3.0.0-preview.1",
  "description": "Responsive grid components for `styled-components` ðŸ’….",
  "keywords": [
    "react",
    "styled-components",
    "grid",
    "responsive",
    "style"
  ],
  "repository": "jameslnewell/styled-components-grid",
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "react-create-component-from-tag-prop": "^1.4.0",
    "react-router-dom": "^5.1.2",
    "styled-components-breakpoint": "3.0.0-preview.19"
  },
  "peerDependencies": {
    "@types/react": "^16.9.5",
    "@types/styled-components": "^4.1.19",
    "react": ">= 15 < 17",
    "styled-components": ">= 1 <= 4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@jameslnewell/babel-preset": "^2.0.0",
    "@jameslnewell/editor-config": "^1.0.1",
    "@jameslnewell/eslint-config": "^0.13.3",
    "@jameslnewell/jest-preset-test": "^4.0.0",
    "@jameslnewell/prettier-config": "^1.0.1",
    "@jameslnewell/typescript-config": "^3.0.0",
    "@testing-library/react": "^9.1.4",
    "@types/chroma-js": "^1.4.3",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "chroma-js": "^2.0.6",
    "eslint": "^6.3.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.3",
    "lint-staged": "^9.2.5",
    "microbundle": "^0.11.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": ">=15",
    "react-dom": "^16.9.0",
    "react-live": "^2.2.1",
    "styled-components": "^4.3.2",
    "styled-components-spacing": "^4.0.0-preview.3",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "clean": "rm -rf dist .cache .rts2_cache* example/dist example.cache",
    "check:linting": "eslint --fix --ext .ts,.js,.jsx src",
    "check:typing:package": "tsc --noEmit --project tsconfig.json",
    "check:typing:example": "tsc --noEmit --project example/tsconfig.json",
    "check:typing": "yarn run check:typing:package && yarn run check:typing:example",
    "check": "yarn run check:linting && yarn run check:typing",
    "build:bundle": "microbundle build -f es,cjs --tsconfig tsconfig.build.json",
    "build:example": "parcel build example/src/index.html --public-url /styled-components-spacing --out-dir example/dist",
    "build": "yarn run build:bundle && yarn run build:example",
    "test": "jest",
    "deploy": "gh-pages -d example/dist",
    "start": "parcel example/src/index.html --out-dir example/dist",
    "prepublishOnly": "yarn run clean && yarn run check && yarn run build && yarn run test && yarn run deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,json,js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT"
}
